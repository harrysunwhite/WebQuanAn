@model WebQuanAn.Models.AdminUser

<div>
    <h4>AdminUser</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Họ và Tên
        </dt>
        <dd class="col-sm-10">
            @Model.Ho&nbsp;@Model.Ten
        </dd>


        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Hinh)
        </dt>
        <dd class="col-sm-10">
            <img src="images/@Model.Hinh" width="150" height="150" alt="">
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.SDT)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SDT)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Role)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Role)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TrangThai)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TrangThai)
        </dd>
    </dl>
</div>

